1. 
a = 2
b = 3
print(a ** b)

2.
base = int(input("Enter base: "))
power = int(input("Enter power: "))
print(base ** power)

3.
num = 4

square = num ** 2
cube = num ** 3

print("Square:", square)
print("Cube:", cube)


4.
base = 3
power = 4
result = 1

for i in range(power):
    result *= base

print(result)

5.
num = 16
print(num ** 0.5)
