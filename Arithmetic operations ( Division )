1. 
a = 20
b = 4
print(a / b)

2. 
num1 = int(input("Enter first number: "))
num2 = int(input("Enter second number: "))
result = num1 / num2
print(result)

3. 
a = 17
b = 5
print(a // b)

4. 
a = 17
b = 5
quotient = a // b
remainder = a % b

print("Quotient:", quotient)
print("Remainder:", remainder)

5. 
num1 = 10
num2 = 0

if num2 != 0:
    print(num1 / num2)
else:
    print("Division by zero is not allowed")
