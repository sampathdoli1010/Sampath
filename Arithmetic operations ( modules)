2.
num1 = int(input("Enter first number: "))
num2 = int(input("Enter second number: "))
print(num1 % num2)


3.
num = 9

if num % 2 == 0:
    print("Even number")
else:
    print("Odd number")

4.
for i in range(1, 11):
    if i % 3 == 0:
        print(i)

5.
days = ["Mon", "Tue", "Wed", "Thu", "Fri"]

for i in range(10):
    print(days[i % 5])
